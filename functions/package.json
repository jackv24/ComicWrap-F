{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "rm -rf lib && tsc",
    "serve": "npm run build && firebase emulators:start --only functions",
    "serve-all": "npm run build && firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "shell-debug": "npm run build && firebase functions:shell --inspect-functions",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "npm run build && mocha --recursive \"./lib/*.test.js\" --reporter spec"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "cheerio": "^1.0.0-rc.5",
    "firebase-admin": "^9.9.0",
    "firebase-functions": "^3.14.1",
    "google-auth-library": "^6.1.4",
    "url-exist": "^2.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/sinon": "^9.0.10",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.8.0",
    "chai": "^4.2.0",
    "eslint": "^7.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.0",
    "firebase-functions-test": "^0.2.0",
    "mocha": "^8.2.1",
    "sinon": "^9.2.3",
    "typescript": "^3.8.0"
  },
  "private": true
}
